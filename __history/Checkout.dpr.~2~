program Checkout;

uses
  Vcl.Forms,
  Checkout.View in 'src\view\Checkout.View.pas' {Form1},
  Menu.View in 'src\view\Menu.View.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
